  "scripts": {
    "dev": "npm run dev:all",
    "dev:all": "concurrently -n=vite,main -c=#646cff,#74b1be \"npm run dev:vite\" \"nest start --watch --preserveWatchOutput\"",
    "dev:vite": "vite",
    "dev:main": "nest start --watch --preserveWatchOutput",
    "build:vite": "vite build",
    "build:main": "rimraf dist nest build",
    "build": "npm run build:vite && NODE_ENV=production nest build",
    "nest": "nest",
    "seed": "ts-node -r tsconfig-paths/register nest/seed.ts",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"nest/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev:hot": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/main/main.js",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --collect-coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --setupFiles dotenv-flow/config",
    "console": "ts-node -r tsconfig-paths/register nest/server/console.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "client:generate:schema": "zeus nest/schema.gql nest/client/app/types --ts"
  },
